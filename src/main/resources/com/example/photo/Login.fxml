    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label prefHeight="17.0" prefWidth="150.0" text="Photo Login">
      <font>
         <Font size="23.0" />
      </font>
   </Label>
   <Pane prefHeight="91.0" prefWidth="180.0">
      <children>
         <Button layoutX="196.0" layoutY="48.0" mnemonicParsing="false" onAction="#onLoginButtonClick" text="Login" />
         <Label layoutY="3.0" text="Username:">
            <font>
               <Font size="17.0" />
            </font>
         </Label>
         <TextArea fx:id="userNameTF" layoutX="86.0" prefHeight="28.0" prefWidth="142.0" />
      </children>
   </Pane>
</VBox>

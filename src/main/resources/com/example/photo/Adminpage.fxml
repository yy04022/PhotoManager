<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.photo.AdminController">
   <children>
      <VBox prefHeight="387.0" prefWidth="600.0">
         <children>
            <Pane prefHeight="128.0" prefWidth="600.0">
               <children>
                  <Label layoutX="50.0" layoutY="15.0" prefHeight="37.0" prefWidth="202.0" text="Admin Control">
                     <font>
                        <Font size="30.0" />
                     </font>
                  </Label>
                  <Button layoutX="460.0" layoutY="90.0" mnemonicParsing="false" onAction="#deleteUserButtonClick" prefHeight="37.0" prefWidth="98.0" text="Delte User" />
                  <Button layoutX="460.0" layoutY="46.0" mnemonicParsing="false" onAction="#listUserButtonClick" prefHeight="37.0" prefWidth="98.0" text="List User" />
                  <Button layoutX="294.0" layoutY="63.0" mnemonicParsing="false" onAction="#addUserButtonClick" prefHeight="47.0" prefWidth="90.0" text="Add User" />
                  <TextField fx:id="userNameTF" layoutX="50.0" layoutY="61.0" prefHeight="47.0" prefWidth="224.0" />
                  <Button layoutX="490.0" layoutY="8.0" mnemonicParsing="false" onAction="#onLogoutButtonClick" text="Log Out" />
               </children>
            </Pane>
            <Pane prefHeight="237.0" prefWidth="600.0">
               <children>
                  <ListView fx:id="userList" layoutX="31.0" layoutY="14.0" prefHeight="221.0" prefWidth="524.0" />
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</AnchorPane>
